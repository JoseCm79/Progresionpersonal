<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/tropa_normal"
    tools:context=".SecondActivity">

    <ImageButton
        android:id="@+id/metasTropa"
        android:layout_width="214dp"
        android:layout_height="48dp"
        android:layout_marginStart="98dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="99dp"
        android:background="@drawable/metas"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="468dp"
        android:background="@drawable/background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/persona_Space"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="61dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="591dp"
        android:fontFamily="@font/avenirblack"
        android:text="José Andrés Carmona Miranda"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="632dp"
        android:fontFamily="@font/avenirblack"
        android:text="@string/InicioTropa"
        android:textColor="@color/letra_aso_azul"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="214dp"
        android:layout_height="48dp"
        android:layout_marginStart="98dp"
        android:layout_marginTop="181dp"
        android:layout_marginEnd="99dp"
        android:background="@drawable/brujulas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="214dp"
        android:layout_height="48dp"
        android:layout_marginStart="98dp"
        android:layout_marginTop="264dp"
        android:layout_marginEnd="99dp"
        android:background="@drawable/bitacoras"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="214dp"
        android:layout_height="48dp"
        android:layout_marginStart="98dp"
        android:layout_marginTop="341dp"
        android:layout_marginEnd="99dp"
        android:background="@drawable/etapas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>


